{
    "name": "echointel/laravel-sdk",
    "description": "Laravel SDK for EchoIntel AI API - Forecasting, customer segmentation, inventory optimization, and more",
    "keywords": [
        "echointel",
        "echosistema",
        "laravel",
        "ai",
        "machine-learning",
        "forecasting",
        "customer-segmentation",
        "inventory",
        "analytics",
        "sdk"
    ],
    "homepage": "https://github.com/EchoSistema/echointel-laravel-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "EchoSistema",
            "email": "dev@echosistema.com",
            "homepage": "https://echosistema.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "EchoIntel\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EchoIntel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EchoIntel\\EchoIntelServiceProvider"
            ],
            "aliases": {
                "EchoIntel": "EchoIntel\\Facades\\EchoIntel"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}